{
  "name": "iasql",
  "version": "0.0.1",
  "description": "Infrastructure as SQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist",
    "lint": "tslint --project .",
    "test": "jest -i",
    "coverage": "jest -i --coverage",
    "coverage:local": "export $(cat .env | xargs) && yarn coverage",
    "start": "if [ \"$IASQL_ENV\" = \"local\" ]; then yarn start:dev; else yarn serve; fi",
    "serve": "node --max_old_space_size=8192 dist/index.js",
    "start:dev": "nodemon -e ts -w ./src -x yarn start:dev:watch",
    "start:dev:watch": "yarn build && yarn serve",
    "gen-sql": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "gen-module": "./.module_migration.sh",
    "docker-compose:dev": "docker-compose --env-file .env -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker-compose": "docker-compose --env-file .env -f docker-compose.yml up --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alantech/iasql.git"
  },
  "keywords": [
    "iasql",
    "devops",
    "iac"
  ],
  "author": "David Ellis <david@alantechnologies.com>",
  "bugs": {
    "url": "https://github.com/alantech/iasql/issues"
  },
  "homepage": "https://iasql.com",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.1",
    "@types/semver": "^7.3.9",
    "jest": "^27.5.1",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.54.0",
    "@aws-sdk/client-ec2": "^3.54.0",
    "@aws-sdk/client-ecr": "^3.41.0",
    "@aws-sdk/client-ecr-public": "^3.43.0",
    "@aws-sdk/client-ecs": "^3.54.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.54.0",
    "@aws-sdk/client-rds": "^3.43.0",
    "@aws-sdk/util-waiter": "^3.32.0",
    "@sentry/node": "^6.16.1",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "axios": "^0.24.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "jwks-rsa": "^2.0.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.5",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0",
    "unique-names-generator": "^4.7.1"
  }
}
