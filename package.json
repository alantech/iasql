{
  "name": "iasql",
  "version": "0.0.10",
  "description": "Infrastructure as SQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc && copyfiles -u 1 src/**/*.sql dist/",
    "clean": "rm -rf dist",
    "lint": "tslint --project .",
    "test": "IASQL_ENV=test jest -i",
    "coverage": "IASQL_ENV=test jest -i --coverage --forceExit --bail",
    "coverage:local": "IASQL_ENV=test export $(cat .env | xargs) && yarn coverage",
    "serve": "node --max_old_space_size=8192 dist/index.js",
    "serve:debug": "node --inspect=0.0.0.0:9229 --max_old_space_size=8192 dist/index.js",
    "start": "yarn serve",
    "start:debug": "nodemon -e ts -w ./src -x yarn serve:debug",
    "gen-sql": "IASQL_ENV=local ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate": "IASQL_ENV=local ts-node ./node_modules/.bin/typeorm migration:run",
    "gen-module": "IASQL_ENV=local ./.module_migration.sh",
    "docker-compose:debug": "IASQL_ENV=local docker-compose -f docker-compose.yml -f docker-compose.debug.yml up --build",
    "docker-compose": "IASQL_ENV=local docker-compose up --build",
    "version": "./.version.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alantech/iasql.git"
  },
  "keywords": [
    "iasql",
    "devops",
    "iac"
  ],
  "author": "David Ellis <david@alantechnologies.com>",
  "bugs": {
    "url": "https://github.com/alantech/iasql/issues"
  },
  "homepage": "https://iasql.com",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.28",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@amplitude/node": "^1.10.0",
    "@aws-sdk/client-acm": "^3.105.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.105.0",
    "@aws-sdk/client-ec2": "^3.112.0",
    "@aws-sdk/client-ecr": "^3.112.0",
    "@aws-sdk/client-ecr-public": "^3.100.0",
    "@aws-sdk/client-ecs": "^3.100.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.121.0",
    "@aws-sdk/client-iam": "^3.100.0",
    "@aws-sdk/client-rds": "^3.100.0",
    "@aws-sdk/client-route-53": "^3.118.0",
    "@aws-sdk/client-s3": "^3.118.0",
    "@aws-sdk/util-waiter": "^3.78.0",
    "@logdna/logger": "^2.6.6",
    "@sentry/node": "^7.5.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-jwt": "^6.1.0",
    "fastest-levenshtein": "^1.0.12",
    "graphile-worker": "^0.13.0",
    "jwks-rsa": "^2.1.4",
    "pg": "^8.7.3",
    "postgraphile": "^4.12.9",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  }
}
