{
  "name": "iasql",
  "version": "0.0.18",
  "description": "Infrastructure as SQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc && copyfiles -u 1 src/**/*.sql dist/ && copyfiles -u 1 src/**/*.json dist/",
    "clean": "rm -rf dist",
    "lint": "tslint --project .",
    "test": "IASQL_ENV=test jest -i",
    "test:local": "IASQL_ENV=test export $(cat .env | xargs) && yarn test",
    "coverage": "IASQL_ENV=test jest -i --coverage --forceExit",
    "coverage:local": "IASQL_ENV=test export $(cat .env | xargs) && yarn coverage",
    "serve": "node --max_old_space_size=8192 dist/index.js",
    "serve:debug": "node --inspect=0.0.0.0:9229 --max_old_space_size=8192 dist/index.js",
    "start": "yarn serve",
    "start:debug": "nodemon -e ts -w ./src -x yarn serve:debug",
    "gen-sql": "IASQL_ENV=ci ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate": "IASQL_ENV=ci ts-node ./node_modules/.bin/typeorm migration:run",
    "gen-module": "IASQL_ENV=ci ./.module_migration.sh",
    "docker-compose:debug": "IASQL_ENV=ci docker-compose -f docker-compose.yml -f docker-compose.debug.yml up --build",
    "docker-compose": "IASQL_ENV=local docker-compose up --build",
    "version": "./.version.sh",
    "drop": "./.drop.sh",
    "develop": "./.develop.sh",
    "style": "npx prettier --config .prettierrc 'src/**/*.ts' --check",
    "format": "npx prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alantech/iasql.git"
  },
  "keywords": [
    "iasql",
    "devops",
    "iac"
  ],
  "author": "David Ellis <david@alantechnologies.com>",
  "bugs": {
    "url": "https://github.com/alantech/iasql/issues"
  },
  "homepage": "https://iasql.com",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/callsite": "^1.0.31",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^16.11.28",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@amplitude/node": "^1.10.0",
    "@aws-sdk/client-acm": "^3.150.0",
    "@aws-sdk/client-apigatewayv2": "^3.160.0",
    "@aws-sdk/client-cloudfront": "^3.150.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.154.0",
    "@aws-sdk/client-dynamodb": "^3.154.0",
    "@aws-sdk/client-ec2": "^3.150.0",
    "@aws-sdk/client-ecr": "^3.154.0",
    "@aws-sdk/client-ecr-public": "^3.163.0",
    "@aws-sdk/client-ecs": "^3.154.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.121.0",
    "@aws-sdk/client-elasticache": "^3.154.0",
    "@aws-sdk/client-iam": "^3.163.0",
    "@aws-sdk/client-lambda": "^3.131.0",
    "@aws-sdk/client-memorydb": "^3.154.0",
    "@aws-sdk/client-rds": "^3.136.0",
    "@aws-sdk/client-route-53": "^3.118.0",
    "@aws-sdk/client-s3": "^3.131.0",
    "@aws-sdk/client-secrets-manager": "^3.159.0",
    "@aws-sdk/client-ssm": "^3.128.0",
    "@aws-sdk/middleware-retry": "^3.160.0",
    "@aws-sdk/util-waiter": "^3.162.0",
    "@logdna/logger": "^2.6.6",
    "@sentry/node": "^7.12.1",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/lodash.pick": "^4.4.7",
    "@types/node-fetch": "^2.6.2",
    "@types/semver": "^7.3.10",
    "axios": "^0.27.2",
    "callsite": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-jwt": "^6.1.0",
    "fastest-levenshtein": "^1.0.14",
    "forever": "^4.0.3",
    "graphile-worker": "^0.13.0",
    "jwks-rsa": "^2.1.4",
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "node-fetch": "^2.6.7",
    "pg": "^8.8.0",
    "pgsql-parser": "^13.3.12",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.7",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.1"
  }
}
