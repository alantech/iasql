{
  "name": "skybase",
  "version": "0.0.1",
  "description": "Skybase Infrastructure as SQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ./src/sql/create-template.sh",
    "clean": "rm -r dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "export $(cat .env | xargs) && node dist/index.js",
    "gen-sql": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alantech/skybase.git"
  },
  "keywords": [
    "skybase",
    "iasql",
    "devops",
    "iac"
  ],
  "author": "David Ellis <david@alantechnologies.com>",
  "bugs": {
    "url": "https://github.com/alantech/skybase/issues"
  },
  "homepage": "https://github.com/alantech/skybase#readme",
  "devDependencies": {
    "@types/node": "^16.9.1",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.31.0",
    "@aws-sdk/util-waiter": "^3.32.0",
    "@types/express": "^4.17.13",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0"
  }
}
