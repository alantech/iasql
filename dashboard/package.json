{
  "name": "dashboard",
  "version": "0.0.26",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@craco/craco": "^6.1.2",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.1",
    "@sentry/react": "^7.4.1",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/lz-string": "^1.3.34",
    "ace-builds": "^1.6.0",
    "lz-string": "^1.4.4",
    "posthog-js": "^1.34.0",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "semver": "^7.3.7",
    "unique-names-generator": "^4.7.1",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "prestart:local": "cd run && yarn build && yarn start:local > out.run.log 2>&1 &",
    "start:local": "FAST_REFRESH=true REACT_APP_IASQL_ENV=local craco --max_old_space_size=8192 start",
    "start": "craco --max_old_space_size=8192 start",
    "start:build": "serve -s build",
    "build": "craco --max_old_space_size=8192 build",
    "lint": "tslint --project .",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 npx playwright test",
    "debug-test": "PWDEBUG=console npx playwright test",
    "gen-test": "npx playwright codegen localhost:3000",
    "eject": "react-scripts eject",
    "style": "npx prettier --config .prettierrc 'src/**/*.{ts,tsx}' --check",
    "format": "npx prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.24.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/amplitude-js": "^8.16.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-table": "^7.7.12",
    "@types/semver": "^7.3.9",
    "autoprefixer": "^9.8.6",
    "dotenv": "^16.0.0",
    "postcss": "^7.0.36",
    "prettier": "^2.7.1",
    "react-error-overlay": "6.0.9",
    "serve": "^13.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
