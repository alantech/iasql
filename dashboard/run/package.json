{
  "name": "run",
  "version": "0.0.23",
  "description": "Express server to bridge IaSQL and Dashboard",
  "main": "dist/index.js",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist",
    "start:local": "IASQL_ENV=local node ./dist/index.js",
    "start": "while :; do node ./dist/index.js; done",
    "docker-build": "docker build -t run:latest .",
    "docker-run": "docker run -p 8888:8888 --name run run:latest"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/uuid": "^9.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@logdna/logger": "^2.6.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.0",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "pgsql-parser": "^13.4.0",
    "typeorm": "^0.2.37",
    "uuid": "^9.0.0"
  }
}
